<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.csc413.team5.fud5.ResultPageActivityFragment">

    <TextView
        android:text="@string/how_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/how_about"
        android:textSize="24sp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Bob&apos;s Burgers"
        android:textSize="34sp"
        android:id="@+id/textView4"
        android:textStyle="bold"
        android:layout_below="@+id/how_about"
        android:layout_centerHorizontal="true" />


    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="100pt"
        android:id="@+id/map"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        tools:context="com.csc413.team5.fud5.MapsActivity"
        android:name="com.google.android.gms.maps.SupportMapFragment" />


    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"></GridLayout>


</RelativeLayout>
