<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.csc413.team5.appdbtest" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AppDbTestActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GreenListActivity"
            android:label="@string/title_activity_green_list"
            android:parentActivityName=".AppDbTestActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.csc413.team5.appdbtest.AppDbTestActivity" />
        </activity>
        <activity
            android:name=".YellowListActivity"
            android:label="@string/title_activity_yellow_list"
            android:parentActivityName=".AppDbTestActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.csc413.team5.appdbtest.AppDbTestActivity" />
        </activity>
        <activity
            android:name=".RedListActivity"
            android:label="@string/title_activity_red_list"
            android:parentActivityName=".AppDbTestActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.csc413.team5.appdbtest.AppDbTestActivity" />
        </activity>
    </application>

</manifest>
